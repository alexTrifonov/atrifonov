<hibernate-mapping>
    <class name="ru.job4j.carstore.AutoModel" table="auto_models">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="model" column="model"/>

        <many-to-one name="makeCar"
                     column="make_id"
                     class="ru.job4j.carstore.MakeCar"/>
    </class>
</hibernate-mapping>